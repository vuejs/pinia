{
  "name": "@pinia/nuxt",
  "version": "0.1.7",
  "description": "Nuxt Module for pinia",
  "keywords": [
    "pinia",
    "nuxt",
    "vue",
    "vuex",
    "store"
  ],
  "homepage": "https://github.com/vuejs/pinia/tree/v2/packages/nuxt#readme",
  "bugs": {
    "url": "https://github.com/vuejs/pinia/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vuejs/pinia.git"
  },
  "funding": "https://github.com/sponsors/posva",
  "license": "MIT",
  "author": {
    "name": "Eduardo San Martin Morote",
    "email": "posva13@gmail.com"
  },
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./dist/module.mjs",
      "require": "./module.cjs"
    },
    "./dist/*": "./dist/*"
  },
  "main": "./module.cjs",
  "types": "./dist/module.d.ts",
  "files": [
    "module.cjs",
    "dist/*.js",
    "dist/*.mjs",
    "dist/*.d.ts"
  ],
  "scripts": {
    "build": "unbuild",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s --commit-path . -l @pinia/nuxt -r 1"
  },
  "dependencies": {
    "vue-demi": "*"
  },
  "devDependencies": {
    "@nuxt/types": "^2.15.8",
    "pinia": "^2.0.0",
    "unbuild": "^0.6.3"
  },
  "peerDependencies": {
    "pinia": "~2.0.7"
  },
  "publishConfig": {
    "access": "public"
  }
}
